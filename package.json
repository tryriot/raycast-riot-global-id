{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "riot-global-id",
  "title": "Riot Global ID Introspection",
  "description": "Introspect Global ID retrieved from the Riot platform",
  "icon": "command-icon.png",
  "author": "pierre-yves",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Introspect a Riot Global ID",
      "description": "Introspect a Global ID to retrieve the underlying object type and ID.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "@tryriot/global-id": "^0.0.15"
  },
  "devDependencies": {
    "@types/node": "~18.15.10",
    "@types/react": "^18.0.30",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}